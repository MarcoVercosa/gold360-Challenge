{
    "swagger": "2.0",
    "info": {
        "description": "System for authenticity logins, registration creation and cancellation",
        "version": "1.0.0",
        "title": "API REGISTER",
        "contact": {
            "email": "marco2011sky@gmail.com"
        }
    },
    "host": "localhost",
    "tags": [
        {
            "name": "Login",
            "description": "Receive token with permission or not",
            "externalDocs": {
                "description": "Find out more",
                "url": "/login"
            }
        },
        {
            "name": "Create / Update Register",
            "description": "Allow to create or change data register",
            "externalDocs": {
                "description": "Find out more",
                "url": "/register"
            }
        },
        {
            "name": "Cancel register",
            "description": "Allow cancel register data (if register is enabled)",
            "externalDocs": {
                "description": "Find out more",
                "url": "/cancel"
            }
        }
    ],
    "schemes": [
        "http"
    ],
    "paths": {
        "/login": {
            "post": {
                "tags": [
                    "login"
                ],
                "summary": "Generate token with permission",
                "description": "This route receive: email and password",
                "operationId": "Login / Password",
                "produces": [
                    "application/json"
                ],
                "parameters": [
                    {
                        "in": "body",
                        "name": "Credentials",
                        "schema": {
                            "type": "object",
                            "properties": {
                                "email": {
                                    "type": "string",
                                    "required": true,
                                    "default": "register@gmail.com"
                                },
                                "password": {
                                    "type": "string",
                                    "required": true,
                                    "default": "Register@123456"
                                }
                            }
                        }
                    }
                ],
                "responses": {
                    "200": {
                        "description": "SUCESS - token generate and sent",
                        "schema": {
                            "$ref": "#/definitions/login_200"
                        }
                    },
                    "401": {
                        "description": " UNAUTHORIZED - User or password is incorrect",
                        "schema": {
                            "$ref": "#/definitions/login_401"
                        }
                    },
                    "500": {
                        "description": " UNEXPECTED ERROR - Internal server error",
                        "schema": {
                            "$ref": "#/definitions/login_500"
                        }
                    }
                }
            }
        },
        "/register": {
            "post": {
                "tags": [
                    "register"
                ],
                "summary": "Returns message informing the result of the operation",
                "description": "This route receive: fullName, email, password to create a new register or update it and Token on header for authentication  ",
                "operationId": "Create / Update Register",
                "produces": [
                    "application/json"
                ],
                "parameters": [
                    {
                        "in": "body",
                        "name": "New register",
                        "required": true,
                        "schema": {
                            "type": "object",
                            "properties": {
                                "fullName": {
                                    "type": "string",
                                    "required": true,
                                    "default": "Registro Teste Registro da Silva"
                                },
                                "email": {
                                    "type": "string",
                                    "required": true,
                                    "default": "registroForever@registro.com"
                                },
                                "password": {
                                    "type": "string",
                                    "required": true,
                                    "default": "RegistroTeste@654321"
                                }
                            }
                        }
                    },
                    {
                        "name": "token",
                        "in": "header",
                        "description": "x-access-token",
                        "required": false,
                        "type": "string",
                        "schema": {
                            "type": "string"
                        }
                    }
                ],
                "responses": {
                    "200": {
                        "description": "SUCESS - Message result",
                        "schema": {
                            "$ref": "#/definitions/register_200"
                        }
                    },
                    "401": {
                        "description": " UNAUTHORIZED - invalid credentials / token",
                        "schema": {
                            "$ref": "#/definitions/register_401"
                        }
                    },
                    "500": {
                        "description": " UNEXPECTED ERROR - Internal server error",
                        "schema": {
                            "$ref": "#/definitions/register_500"
                        }
                    }
                },
                "security": [
                    {
                        "tokenAuth": []
                    }
                ]
            }
        },
        "/cancel": {
            "post": {
                "tags": [
                    "cancel"
                ],
                "summary": "Cancel register if enable",
                "description": "This route receive: full name, email and Token on header for authentication",
                "operationId": "Cancel register ",
                "produces": [
                    "application/json"
                ],
                "parameters": [
                    {
                        "in": "body",
                        "name": "New register",
                        "required": true,
                        "schema": {
                            "type": "object",
                            "properties": {
                                "fullName": {
                                    "type": "string",
                                    "required": true,
                                    "default": "Registro Teste Registro da Silva"
                                },
                                "email": {
                                    "type": "string",
                                    "required": true,
                                    "default": "registroForever@registro.com"
                                }
                            }
                        }
                    }
                ],
                "responses": {
                    "200": {
                        "description": "SUCESS - Message result",
                        "schema": {
                            "$ref": "#/definitions/cancel_200"
                        }
                    },
                    "401": {
                        "description": " UNAUTHORIZED - invalid credentials / token",
                        "schema": {
                            "$ref": "#/definitions/cancel_401"
                        }
                    },
                    "500": {
                        "description": " UNEXPECTED ERROR - Internal server error",
                        "schema": {
                            "$ref": "#/definitions/cancel_500"
                        }
                    }
                },
                "security": [
                    {
                        "tokenAuth": []
                    }
                ]
            }
        }
    },
    "securityDefinitions": {
        "tokenAuth": {
            "type": "apiKey",
            "name": "x-access-token",
            "in": "header"
        }
    },
    "definitions": {
        "login_200": {
            "type": "object",
            "properties": {
                "sucess": {
                    "type": "boolean"
                },
                "token": {
                    "type": "string"
                },
                "result": {
                    "type": "object",
                    "properties": {
                        "id": {
                            "type": "integer"
                        },
                        "fullName": {
                            "type": "string"
                        },
                        "email": {
                            "type": "string"
                        },
                        "lastLogin": {
                            "type": "string"
                        }
                    }
                }
            },
            "application/json": {
                "name": "register"
            }
        },
        "login_401": {
            "type": "object",
            "properties": {
                "sucess": {
                    "type": "boolean",
                    "default": "false"
                },
                "token": {
                    "type": "string"
                },
                "result": {
                    "type": "string"
                }
            },
            "application/json": {
                "name": "register"
            }
        },
        "login_500": {
            "type": "object",
            "properties": {
                "statusCode": {
                    "type": "integer",
                    "default": "500"
                },
                "error": {
                    "type": "string",
                    "default": "Internal server error"
                },
                "message": {
                    "type": "string"
                }
            },
            "application/json": {
                "name": "register"
            }
        },
        "register_200": {
            "type": "object",
            "properties": {
                "sucess": {
                    "type": "boolean"
                },
                "token": {
                    "type": "string"
                },
                "result": {
                    "type": "object",
                    "properties": {
                        "sucess": {
                            "type": "boolean"
                        },
                        "message": {
                            "type": "string"
                        }
                    }
                }
            },
            "application/json": {
                "name": "register"
            }
        },
        "register_401": {
            "type": "object",
            "properties": {
                "sucess": {
                    "type": "boolean",
                    "default": "false"
                },
                "token": {
                    "type": "string"
                },
                "result": {
