                "name": "register"
            }
        },
        "register_500": {
            "type": "object",
            "properties": {
                "statusCode": {
                    "type": "integer",
                    "default": "500"
                },
                "error": {
                    "type": "string",
                    "default": "Internal server error"
                },
                "message": {
                    "type": "string"
                }
            },
            "application/json": {
                "name": "register"
            }
        },
        "cancel_200": {
            "type": "object",
            "properties": {
                "sucess": {
                    "type": "boolean"
                },
                "token": {
                    "type": "string"
                },
                "result": {
                    "type": "object",
                    "properties": {
                        "sucess": {
                            "type": "boolean"
                        },
                        "message": {
                            "type": "string"
                        }
                    }
                }
            },
            "application/json": {
                "name": "retornanumeroshinoharpa"
            }
        },
        "cancel_401": {
            "type": "object",
            "properties": {
                "sucess": {
                    "type": "boolean",
                    "default": "false"
                },
                "token": {
                    "type": "string"
                },
                "result": {
                    "type": "string"
                }
            },
            "application/json": {
                "name": "retornanumeroshinoharpa"
            }
        },
        "cancel_500": {
            "type": "object",
            "properties": {
                "statusCode": {
                    "type": "integer",
                    "default": "500"
                },
                "error": {
                    "type": "string",
                    "default": "Internal server error"
                },
                "message": {
                    "type": "string"
                }
            },
            "application/json": {
                "name": "retornanumeroshinoharpa"
            }
        }
    }
}